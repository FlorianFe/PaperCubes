{
  "name": "PaperCubes",
  "version": "1.2.0",
  "description": "Electron app for building blueprints for minecraft origami",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "compile:win32": "cd ./build/minecraftOrthogami/ && node-gyp rebuild --target=1.7.8 --arch=x64 --dist-url=https://atom.io/download/electron && copy -r ./build/Release/ ../../main-process/calculateOrigami/minecraftOrthogami/ ",
    "compile": "cd ./build/minecraftOrthogami/ && node-gyp rebuild --target=1.7.8 --arch=x64 --dist-url=https://atom.io/download/electron && cp -r ./build/Release/ ../../main-process/calculateOrigami/minecraftOrthogami/ ",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "de.florianfe.papercubes",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icon@2x"
    },
    "win": {
      "icon": "build/icon"
    }
  },
  "author": "Florian Fechner",
  "license": "CC BY-NC",
  "dependencies": {
    "handlebars": "^4.0.11",
    "nbt": "^0.7.0",
    "unzip": "^0.1.11"
  },
  "devDependencies": {
    "electron": "1.7.8",
    "electron-builder": "^19.49.3",
    "node-gyp": "^3.6.2",
    "run-script-os": "^1.0.3"
  }
}
